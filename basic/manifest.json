{
    "manifest_version": 2,
    "name": "Prateek",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js":   ["content.js"
            ]
        }
    ],
    "background": {
        "scripts": [
          "background.js"
        ],
        "persistent": false
      },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions":[
        "storage",
        "notifications"
    ]
}